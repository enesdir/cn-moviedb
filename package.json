  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.4",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.35",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
